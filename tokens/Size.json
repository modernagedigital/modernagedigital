{
  "font-size": {
    "xs": {
      "$type": "fontSizes",
      "$value": "({type.max.xs} - {type.min.xs}) * {screen-location} + {type.min.xs}"
    },
    "sm": {
      "$type": "fontSizes",
      "$value": "({type.max.sm} - {type.min.sm}) * {screen-location} + {type.min.sm}"
    },
    "base": {
      "$type": "fontSizes",
      "$value": "({type.max.base} - {type.min.base}) * {screen-location} + {type.min.base}"
    },
    "lg": {
      "$type": "fontSizes",
      "$value": "({type.max.lg} - {type.min.lg}) * {screen-location} + {type.min.lg}"
    },
    "xl": {
      "$type": "fontSizes",
      "$value": "({type.max.xl} - {type.min.xl}) * {screen-location} + {type.min.xl}"
    },
    "2xl": {
      "$type": "fontSizes",
      "$value": "({type.max.2xl} - {type.min.2xl}) * {screen-location} + {type.min.2xl}"
    },
    "3xl": {
      "$type": "fontSizes",
      "$value": "({type.max.3xl} - {type.min.3xl}) * {screen-location} + {type.min.3xl}"
    },
    "4xl": {
      "$type": "fontSizes",
      "$value": "({type.max.4xl} - {type.min.4xl}) * {screen-location} + {type.min.4xl}"
    },
    "5xl": {
      "$type": "fontSizes",
      "$value": "({type.max.5xl} - {type.min.5xl}) * {screen-location} + {type.min.5xl}"
    },
    "6xl": {
      "$type": "fontSizes",
      "$value": "({type.max.6xl} - {type.min.6xl}) * {screen-location} + {type.min.6xl}"
    },
    "7xl": {
      "$type": "fontSizes",
      "$value": "({type.max.7xl} - {type.min.7xl}) * {screen-location} + {type.min.7xl}"
    }
  },
  "size": {
    "4xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.25"
    },
    "3xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.375"
    },
    "2xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.5"
    },
    "xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.75"
    },
    "sm": {
      "$type": "dimension",
      "$value": "{space-calc} * 1"
    },
    "md": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.25"
    },
    "lg": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.5"
    },
    "xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.75"
    },
    "2xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2"
    },
    "3xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.25"
    },
    "4xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.5"
    },
    "5xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.75"
    },
    "6xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 3"
    },
    "7xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 3.5"
    },
    "8xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 4"
    },
    "9xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 4.5"
    },
    "10xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 5"
    },
    "12xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 6"
    },
    "14xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 7"
    },
    "xs-md": {
      "$type": "dimension",
      "$value": "(((({size.md} - {size.xs}) * {screen-location}) + {size.xs}))"
    },
    "xs-sm": {
      "$type": "dimension",
      "$value": "(((({size.sm} - {size.xs}) * {screen-location}) + {size.xs}))"
    },
    "sm-md": {
      "$type": "dimension",
      "$value": "(((({size.md} - {size.sm}) * {screen-location}) + {size.sm}))"
    },
    "sm-lg": {
      "$type": "dimension",
      "$value": "(((({size.lg} - {size.sm}) * {screen-location}) + {size.sm}))"
    }
  }
}