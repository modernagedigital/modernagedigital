{
  "Font-Size": {
    "xs": {
      "$type": "fontSizes",
      "$value": "12"
    },
    "sm": {
      "$type": "fontSizes",
      "$value": "({type.max.sm} - {type.min.sm}) * {screen-location} + {type.min.sm}"
    },
    "base": {
      "$type": "fontSizes",
      "$value": "({type.max.base} - {type.min.base}) * {screen-location} + {type.min.base}"
    },
    "lg": {
      "$type": "fontSizes",
      "$value": "({type.max.lg} - {type.min.lg}) * {screen-location} + {type.min.lg}"
    },
    "xl": {
      "$type": "fontSizes",
      "$value": "({type.max.xl} - {type.min.xl}) * {screen-location} + {type.min.xl}"
    },
    "2xl": {
      "$type": "fontSizes",
      "$value": "({type.max.2xl} - {type.min.2xl}) * {screen-location} + {type.min.2xl}"
    },
    "3xl": {
      "$type": "fontSizes",
      "$value": "({type.max.3xl} - {type.min.3xl}) * {screen-location} + {type.min.3xl}"
    },
    "4xl": {
      "$type": "fontSizes",
      "$value": "({type.max.4xl} - {type.min.4xl}) * {screen-location} + {type.min.4xl}"
    },
    "5xl": {
      "$type": "fontSizes",
      "$value": "({type.max.5xl} - {type.min.5xl}) * {screen-location} + {type.min.5xl}"
    },
    "6xl": {
      "$type": "fontSizes",
      "$value": "({type.max.6xl} - {type.min.6xl}) * {screen-location} + {type.min.6xl}"
    },
    "7xl": {
      "$type": "fontSizes",
      "$value": "({type.max.7xl} - {type.min.7xl}) * {screen-location} + {type.min.7xl}"
    }
  },
  "Size": {
    "4xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.25"
    },
    "3xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.375"
    },
    "2xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.5"
    },
    "xs": {
      "$type": "dimension",
      "$value": "{space-calc} * 0.75"
    },
    "sm": {
      "$type": "dimension",
      "$value": "{space-calc} * 1"
    },
    "md": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.25"
    },
    "lg": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.5"
    },
    "xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 1.75"
    },
    "2xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2"
    },
    "3xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.25"
    },
    "4xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.5"
    },
    "5xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 2.75"
    },
    "6xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 3"
    },
    "7xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 3.5"
    },
    "8xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 4"
    },
    "9xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 5"
    },
    "10xl": {
      "$type": "dimension",
      "$value": "{space-calc} * 6"
    },
    "xs-md": {
      "$type": "dimension",
      "$value": "(((({Size.md} - {Size.xs}) * {screen-location}) + {Size.xs}))"
    },
    "xs-sm": {
      "$type": "dimension",
      "$value": "(((({Size.sm} - {Size.xs}) * {screen-location}) + {Size.xs}))"
    },
    "sm-md": {
      "$type": "dimension",
      "$value": "(((({Size.md} - {Size.sm}) * {screen-location}) + {Size.sm}))"
    },
    "sm-lg": {
      "$type": "dimension",
      "$value": "(((({Size.lg} - {Size.sm}) * {screen-location}) + {Size.sm}))"
    }
  },
  "Container": {
    "xs": {
      "$type": "dimension",
      "$value": "30rem",
      "$description": "480px"
    },
    "sm": {
      "$type": "dimension",
      "$value": "40rem",
      "$description": "640px"
    },
    "md": {
      "$type": "dimension",
      "$value": "48rem",
      "$description": "768px"
    },
    "lg": {
      "$type": "dimension",
      "$value": "64rem",
      "$description": "1024px"
    },
    "xl": {
      "$type": "dimension",
      "$value": "80rem",
      "$description": "1280px"
    }
  },
  "Layout": {
    "Pad X": {
      "$type": "dimension",
      "$value": "{Size.sm-lg}"
    },
    "Pad Y": {
      "$type": "dimension",
      "$value": "{Size.xl}"
    },
    "Container-Max": {
      "$type": "dimension",
      "$value": "{Container.xl}",
      "$description": "Max site width"
    },
    "Grid Gap": {
      "$type": "dimension",
      "$value": "{Size.sm}"
    },
    "Grid Columns": {
      "$type": "number",
      "$value": "12"
    },
    "Touch Target": {
      "$type": "dimension",
      "$value": "2.75rem",
      "$description": "48px"
    }
  }
}